{
  "name": "project3",
  "version": "1.0.0",
  "description": "Gift Registry",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:prod": "node server.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keensteven/Project3.git"
  },
  "author": "Steven Keene, Chris Ross, Alicia Tamboli, Vijayalakshmitce",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keensteven/Project3/issues"
  },
  "homepage": "https://github.com/keensteven/Project3#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "reactstrap": "^8.0.1",
    "sequelize": "^5.18.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3"
  }
}
